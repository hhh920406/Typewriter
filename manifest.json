{
  "manifest_version": 2,

  "name": "人人自动点赞",
  "description": "点赞狂魔。",
  "version": "1.0.4",
  
  "icons": {
	"64": "icon.png"
  },
  
  "permissions": [
    "http://renren.com/",
    "http://www.renren.com/",
    "http://guide.renren.com/",
	"tabs",
	"cookies",
	"storage",
	"webNavigation"
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"]
  }
}
